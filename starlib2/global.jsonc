{
    "namespace": "global",
    "binding": {
        "type": "panel",
        "$key|default": "",
        "visible": false,
        "enabled": false,
        "bindings": [
            {
                "binding_collection_name": "form_buttons",
                "binding_name": "#form_button_text",
                "binding_type": "collection"
            },
            {
                "binding_type": "view",
                "source_property_name": "(not((#form_button_text - $key) = #form_button_text))",
                "target_property_name": "#visible"
            },
            {
                "binding_type": "view",
                "source_property_name": "(not((#form_button_text - $key) = #form_button_text))",
                "target_property_name": "#enabled"
            }
        ]
    },
    "reverse_binding": {
        "type": "panel",
        "$key|default": "",
        "visible": true,
        "enabled": true,
        "bindings": [
            {
                "binding_collection_name": "form_buttons",
                "binding_name": "#form_button_text",
                "binding_type": "collection"
            },
            {
                "binding_type": "view",
                "source_property_name": "((#form_button_text - $key) = #form_button_text)",
                "target_property_name": "#visible"
            },
            {
                "binding_type": "view",
                "source_property_name": "((#form_button_text - $key) = #form_button_text)",
                "target_property_name": "#enabled"
            }
        ]
    },
    "title_binding": {
        "type": "panel",
        "$key|default": "",
        "visible": false,
        "enabled": false,
        "bindings": [
            {
                "binding_name": "#title_text",
                "binding_type": "global"
            },
            {
                "binding_type": "view",
                "source_property_name": "(not((#title_text - $key) = #title_text))",
                "target_property_name": "#visible"
            },
            {
                "binding_type": "view",
                "source_property_name": "(not((#title_text - $key) = #title_text))",
                "target_property_name": "#enabled"
            }
        ]
    },
    "reverse_title_binding": {
        "type": "panel",
        "$key|default": "",
        "visible": true,
        "enabled": true,
        "bindings": [
            {
                "binding_name": "#title_text",
                "binding_type": "global"
            },
            {
                "binding_type": "view",
                "source_property_name": "((#title_text - $key) = #title_text)",
                "target_property_name": "#visible"
            },
            {
                "binding_type": "view",
                "source_property_name": "((#title_text - $key) = #title_text)",
                "target_property_name": "#enabled"
            }
        ]
    },
    "range_selection_binding": {
      "type": "panel",
      "$from|default": "",
      "$to|default": "",
      "visible": false,
      "enabled": false,
      "bindings": [
          {
              "binding_collection_name": "form_buttons",
              "binding_name": "#form_button_text",
              "binding_type": "collection"
          },
          {
              "binding_type": "view",
              "source_property_name": "(((('%.' + $to + 's') * #form_button_text) - (('%.' + $from + 's') * #form_button_text)) - '\t')",
              "target_property_name": "#text"
          },
          {
              "source_property_name": "(not(#text = ''))",
              "binding_type": "view",
              "target_property_name": "#visible"
          }
      ]
   },
    "scrolling_panel@common.scrolling_panel": {
        "$scroll_bar_contained|default": false,
        "$scroll_bar_left_padding_size|default": [
          3,
          0
        ],
        "$scroll_bar_right_padding_size|default": [
          0,
          0
        ],
        "$scroll_size|default": [
          5,
          "100%"
        ],
        "$scroll_view_port_offset|default": [
          0,
          0
        ],
        "$scrolling_pane_offset|default": [
          0,
          0
        ],
        "$scrolling_pane_size|default": [
          "100%",
          "100%"
        ],
        "$show_background|default": false,
        "anchor_from": "center",
        "anchor_to": "center",
        "size": [
          "100%",
          "100%"
        ]
    },
    "empty_panel": {
        "type": "panel",
        "size": [
            0,
            0
        ]
    },
    "button_factory": {
        "type": "stack_panel",
        "orientation": "vertical",
        "size": [
          "100%",
          "100%c"
        ],
        "$factory_item|default": "style.dme_04",
        "factory": {
          "name": "buttons",
          "control_name": "$factory_item"
        },
        "collection_name": "form_buttons",
        "bindings": [
            {
              "binding_name": "#form_button_length",
              "binding_name_override": "#collection_length"
            }
        ]
    },
    "grid": {
        "type": "grid",
        "$grid_item|default": "style.dme_05",
        "grid_item_template": "$grid_item",
        "grid_rescaling_type": "horizontal",
        "collection_name": "form_buttons",
        "size": [
          "100%",
          "100%c"
        ],
        "bindings": [
            {
                "binding_name_override": "#maximum_grid_items",
                "binding_name": "#form_button_length"
            }
        ]
    }
}